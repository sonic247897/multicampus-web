		function printGuGu(dan){
			for(i=1; i<=9; ++i){
			document.write(dan +"*"+i+ "=" + dan*i);
			}
		}
		
		function calcNum(num1, num2){
			result = num1+num2;
			return result;
			
		}
	</script>
